ListNode* removeElements(ListNode* head, int val) {
        if (!head) return nullptr;
        while (head && head->val == val){
            ListNode* temp = head;
            head = head->next;
            delete temp;
        }
        ListNode* prev = nullptr;
        ListNode* curr = head;
        while(curr){
            if (curr->val == val){
                prev->next = curr->next;
                ListNode* tem = curr;
                curr=curr->next;
                delete tem;
            }else{
            prev= curr;
            curr=curr->next;
            }
        }
        return head;
    }
